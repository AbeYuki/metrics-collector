{
    "version": 1,
    "formatters": {
        "default": {
            "format": "%(levelprefix)s %(asctime)s %(message)s",
            "datefmt": "%Y-%m-%d %H:%M:%S"
        }
    },
    "handlers": {
        "default": {
            "formatter": "default",
            "class": "logging.StreamHandler",
            "stream": "ext://sys.stderr"
        }
    },
    "loggers": {
        "uvicorn": {
            "handlers": ["default"],
            "level": "INFO"
        },
        "uvicorn.error": {
            "handlers": ["default"],
            "level": "INFO",
            "propagate": false
        },
        "uvicorn.access": {
            "handlers": ["default"],
            "level": "INFO",
            "propagate": false
        }
    }
}